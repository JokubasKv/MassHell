<Window x:Class="MassHell_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MassHell_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="660" Width="1845">
    <Grid x:Name="MapXML" Keyboard.KeyDown="MapXML_KeyDown" Background="AliceBlue">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Connect" Grid.Column="1" Content="Button" HorizontalAlignment="Left" Margin="0,389,0,0" VerticalAlignment="Top" Click="Connect_Click_1"/>
        <Image x:Name="Player" Source="Images/Varn_token.png" Margin="533,79,76,343" >
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform X="0" Y="{Binding newY}"/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Button x:Name="Down_Left" Content="Button" HorizontalAlignment="Left" Margin="340,494,0,0" VerticalAlignment="Top" Click="Down_Left_Click"/>
    </Grid>
</Window>
